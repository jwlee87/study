<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
	</head>
	<body>
		<h1>노드 게시판</h1>
		<table>
		<tr>
		  <th>번호</th>
		  <th>제목</th>
		  <th>작성자</th>
		</tr>
		<%
		rows.forEach(function (row, i) {
		%>
		  <tr>
		    <td><%= row.no %></td>
		    <td><%= row.title %></td>
		    <td><%= row.writer %></td>
		  </tr>
		<%  			
		});
		%>
		</table>
	</body>
</html>		
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ajax.quiz.Exam01DAO">
    <select id="getMemberInfo" parameterType="string" resultType="exam01">
        select id,
        	   name,
        	   email,
        	   addr
          from mybatis_member
         where id = #{id}
    </select>
</mapper>




















